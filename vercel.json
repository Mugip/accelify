{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/__create/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "apps/web/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/apps/web/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/web/__create/index.ts"
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build"
}
